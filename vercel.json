{
  "version": 2,
  "builds": [
    {
      "src": "vercel_handler.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_handler.py"
    }
  ],
  "env": {
    "DISABLE_DNS_CHECKS": "true"
  }
}